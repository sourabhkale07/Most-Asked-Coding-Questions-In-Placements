import java.util.Scanner;

class Main{
public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    
    System.out.println("Enter the number to be reversed");
    
    int num = sc.nextInt();
    int c,no=0;
    
    while(num!=0){
        c = num%10;
        no = no*10 +c;
        num = num/10;
    }
    
    System.out.println(no);
    
}
}
